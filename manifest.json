{
  "manifest_version": 3,
  "name": "Leetcoder AI",
  "version": "1.0.0",
  "description": "Chrome Extension that helps you solve Leetcode problems using AI.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["https://leetcode.com/problems/*"]
    }
  ]
}
